# Path to your oh-my-zsh installation.
export ZSH="/Users/richard/.oh-my-zsh"

ZSH_THEME="agnoster"

plugins=(git fasd)

source $ZSH/oh-my-zsh.sh

eval $(thefuck --alias)
eval "$(thefuck --alias f)"
eval "$(fasd --init auto)"
# User configuration

alias c="clear"

export NVM_DIR=~/.nvm
source $(brew --prefix nvm)/nvm.sh
